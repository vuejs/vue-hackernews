  "scripts": {
    "dev": "webpack --module-bind \"vue=vue\" --watch --devtool source-map src/main.js build/build.js",
    "build": "webpack --module-bind \"vue=vue\" --plugin webpack/lib/optimize/UglifyJsPlugin src/main.js build/build.js"
  },
