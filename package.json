    "url": "https://github.com/yyx990803/vue-hackernews/issues"
  },
  "homepage": "https://github.com/yyx990803/vue-hackernews",
  "devDependencies": {
    "stylus": "^0.50.0",
    "vue-loader": "^1.1.4",
    "webpack": "^1.8.4"
  },
  "dependencies": {
    "director": "^1.2.8",
    "firebase": "^2.2.4",
    "vue": "0.11.5"
  }
